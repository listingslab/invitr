{
  "name": "invitr",
  "version": "1.1.0",
  "author": "Chris Dorward <listingslab@gmail.com> (http://listingslab.com)",
  "description": "Isomorphic JavaScript Invitation App",
  "keywords": [
    "isomorphic",
    "javascript",
    "node.js",
    "Koa",
    "react",
    "webpack",
    "gulp"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:listingslab/invitr.git"
  },
  "main": "start.js",
  "scripts": {
    "start": "node start.js",
    "api": "node server/api_dev.js",
    "build": "clear && webpack --optimize-minimize && node start.js",
    "dev": "clear && webpack-dev-server --port 1968 --inline --progress --hot --content-base public --history-api-fallback",
    "lint": "eslint . --ext .js --ext .jsx || true"
  },
  "dependencies": {
    "clear": "0.0.1",
    "colors": "^1.1.2",
    "koa": "^1.2.0",
    "koa-mount": "^1.3.0",
    "koa-route": "^2.4.2",
    "koa-static": "^2.0.0",
    "lodash": "^4.11.1",
    "moment": "^2.13.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-router": "^2.3.0"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.23.1",
    "del": "^2.2.0",
    "eslint": "^2.7.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^4.3.0",
    "gulp": "^3.9.1",
    "jshint": "^2.9.1",
    "jshint-loader": "^0.8.3",
    "jsx-loader": "^0.13.2",
    "run-sequence": "^1.1.5",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  }
}
